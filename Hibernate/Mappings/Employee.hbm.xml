<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Hibernate"
                   namespace="Hibernate.Domain">

  <class name="Employee" table="Dimension.Employee">
    <id name="EmployeeKey" column="`Employee Key`">
      <generator class="guid"/>
    </id>
    <property name="EmployeeName" column="Employee"/>
    <property name="PreferredName" column="`Preferred Name`"/>
    <property name="IsSalesperson" column="`Is Salesperson`"/>
  </class>
</hibernate-mapping>